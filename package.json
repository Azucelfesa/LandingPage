{
	"name": "landing-page",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"build": "vite build",
		"dev": "vite dev",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"deploy": "npm run build && wrangler pages deploy .svelte-kit/cloudflare",
		"deploy:preview": "npm run build && wrangler pages deploy .svelte-kit/cloudflare --compatibility-date=2024-01-01"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-cloudflare": "^7.2.3",
		"@sveltejs/adapter-static": "^3.0.9",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"svelte": "^4.2.0",
		"svelte-check": "^3.6.0",
		"tsx": "^4.20.5",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"wrangler": "^4.39.0"
	},
	"type": "module",
	"dependencies": {
		"@trpc/client": "^11.5.1",
		"gsap": "^3.13.0",
		"zod": "^4.1.9"
	}
}
